<%- include page/header %>

<!--更新時間:<%=new Date() %> -->
	<div class="row">
		<div class="col-md-12 column">
		<table id="mytabe" class="table table-striped table-hover">
			<% if (user.authz.a03==true) { %>

				<tr bgcolor="#428bca">
					<td>位址</td>
					<td>溫度1</td>
					<td>濕度1</td>
					<td>電壓</td>
					<td>時間</td>
				</tr>

				<% units.forEach(function(unit) { %>
					<tr id="<%= unit.macAddr %>">
						<td><div name ="name"><%= unit.name %></div></td>
						<td><div name ="tmp1"><%= unit.information.temperature %></td>
						<td><div name ="hum1"><%= unit.information.humidity %></td>
						<td><div name ="voltage"><%= unit.information.voltage %></td>
						<td><div name ="time"><%= unit.date %></td>
					</tr>
				<% }); %>
			<% } %>
		</table>
		</div> <!-- column-->
	</div>	<!--row-->

	<div class="row">
		<div class="col-md-12 column">
			<label id="error_message" class="checkbox">
	            <font color="red"><b></b>
	            <% if (user.authz.a03==false) { %>
	                你沒有觀看權限,請通知管理員變更權限!
	            <% } %>
	        </label>
		</div> <!-- column-->
